{% extends 'base.html.twig' %}

{% block title %}All our categories!{% endblock %}

{% block body %}
<div class="example-wrapper">
    <ul>
    {% for category in categories %}
        <li>
            <a href="{{ path("category_show", { categoryName : category.name }) }}">{{ category.name }}</a>
        </li>
            {% endfor %}
    </ul>
</div>
{% endblock %}
